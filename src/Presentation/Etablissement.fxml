            <TableColumn fx:id="Fax" prefWidth="83.0" text="Fax" />
            <TableColumn fx:id="Page_fb" prefWidth="124.0" text="Page facebook" />
            <TableColumn fx:id="Site_web" prefWidth="158.0" text="Site web" />
            <TableColumn fx:id="Heure_ouverture" prefWidth="106.0" text="Heure ouverture" />
            <TableColumn fx:id="Heure_fermeture" prefWidth="100.0" text="Heure fermeture" />
            <TableColumn fx:id="Adresse" minWidth="0.0" prefWidth="139.0" text="Adresse" />
         </columns>
      </TableView>
   </children>
