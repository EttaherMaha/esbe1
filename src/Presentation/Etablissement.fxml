<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.EtablissementController">
   <children>
      <Label layoutX="126.0" layoutY="548.0" text="Veuillez entrer le nom de l'etablissement que vous voudrez chercher : " />
